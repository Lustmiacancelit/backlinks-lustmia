{
  "crons": [
    {
      "path": "/api/cron/reindex",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/toxic-sweeps/run",
      "schedule": "0 */6 * * *"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "backlinks.lustmia.com"
        }
      ],
      "destination": "https://rankcore.ai/$1",
      "permanent": true
    }
  ]
}
